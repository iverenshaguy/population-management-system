{
  "name": "pms",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "engines": {
    "node": "10.7.0"
  },
  "scripts": {
    "start": "dotenv -- node dist/index.js",
    "dev": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "seed": "dotenv -- ts-node src/seeders/index.ts",
    "test": "yarn seed && dotenv -- jest src/tests/tests.test.ts",
    "build": "rimraf dist && tsc",
    "heroku-postbuild": "npm run build",
    "env": "sync-dotenv",
    "codecov": "codecov"
  },
  "dependencies": {
    "@types/jest": "^24.0.15",
    "apollo-errors": "^1.9.0",
    "bcryptjs": "2.4.3",
    "dotenv-cli": "1.4.0",
    "graphql-constraint-directive": "^1.4.0",
    "graphql-yoga": "1.17.4",
    "jsonwebtoken": "8.5.1",
    "mongoose": "^5.6.0",
    "mongoose-autopopulate": "^0.9.1",
    "mongoose-seed": "^0.6.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/node": "^12.0.8",
    "codecov": "^3.6.5",
    "graphql-request": "^1.8.2",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "mocha": "^6.1.4",
    "nodemon": "1.19.1",
    "prisma": "1.34.0",
    "rimraf": "2.6.3",
    "supertest": "^4.0.2",
    "sync-dotenv": "^2.2.0",
    "ts-jest": "^24.0.2",
    "ts-node": "7.0.1",
    "typescript": "3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run env"
    }
  }
}
